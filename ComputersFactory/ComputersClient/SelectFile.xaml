<Window x:Class="ComputersClient.Windows.SelectFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComputersClient.Windows"
        mc:Ignorable="d"
        Title="SelectFile" Height="600" Width="500">
    <Grid>
        <Label x:Name="label" Content="Browse File" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="24" Margin="10,10,0,0"/>
        <Button x:Name="button" Content="Browse" HorizontalAlignment="Left" Margin="10,88,0,0" VerticalAlignment="Top" Width="118" FontSize="20" Height="46" Click="button_Click"/>
        <Label x:Name="label1" Content="FIleName: " HorizontalAlignment="Left" Margin="10,139,0,0" VerticalAlignment="Top" FontSize="10" Width="472"/>
        <Button x:Name="button1" Content="Extract" HorizontalAlignment="Left" Margin="10,194,0,0" VerticalAlignment="Top" Width="118" Click="button1_Click" Height="46" FontSize="20"/>

    </Grid>
</Window>
